{
  "expo": {
    "name": "QuickPreview Example",  
    "slug": "expo-quick-preview-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "quickpreviewdemo",
    "experiments": {
      "typedRoutes": true
    },

    "icon": "./assets/QuickLookLogo.png",
    "splash": {
      "image": "./assets/QuickLookLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/QuickLookLogo.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [{ "scheme": "quickpreviewdemo" }]
        }
      ]
    },

    "web": {
      "favicon": "./assets/favicon.svg"
    },

    "plugins": ["expo-router"],

    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    }
  }
}
