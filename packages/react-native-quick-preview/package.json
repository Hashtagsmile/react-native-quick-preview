{
  "name": "react-native-quick-preview",
  "version": "1.0.5",
  "description": "A beautiful, customizable quick preview modal component for React Native",
  "author": "Oliver Lindblad",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "rimraf dist && tsup"
  },
  "keywords": [
    "react-native",
    "modal",
    "preview",
    "quickpreview",
    "quick-preview",
    "quick-view",
    "quick-view-modal",
    "quick-view-dialog",
    "quick-view-sheet",
    "quick-view-popover",
    "quick-view-drawer",
    "quick-view-modal",
    "quick-view-dialog",
    "quick-view-sheet",
    "quick-view-popover",
    "quick-view-drawer",
    "quick-view-modal",
    "quick-view-dialog",
    "quick-view-sheet",
    "quick-view-popover",
    "quick-view-drawer",
    "expo",
    "typescript",
    "ui",
    "component"
  ],
  "peerDependencies": {
    "react": ">=18.2.0 <21",
    "react-native": ">=0.64.0 <1",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-portalize": ">=1.0.0",
    "react-native-safe-area-context": ">=4.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-portalize": "^1.0.0",
    "react-native-reanimated": "^3.0.0",
    "react-native-safe-area-context": "^4.0.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hashtagsmile/react-native-quick-preview.git"
  },
  "bugs": {
    "url": "https://github.com/Hashtagsmile/react-native-quick-preview/issues"
  },
  "homepage": "https://github.com/Hashtagsmile/react-native-quick-preview#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
